<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mit.lawyered">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        <activity android:name="com.mit.law.view.activity.SignIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mit.law.view.activity.Home"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>

        <activity android:name="com.mit.law.view.activity.Clicks_on_law_dialog"
            android:theme="@style/Theme.AppCompat.Light.Dialog"/>
        <activity android:name="com.mit.law.view.activity.LawDetailsActivity" />
    </application>

</manifest>